auth_student=select email,password from user_info where email=? and password=?
add_book=insert into books_info values (?,?,?,?,?,?,?,?,?)
remove_book=delete from  books_info values where book_id=?
add_user=insert into user_info values (?,?,?,?)
remove_user=delete from user_info where email=?
update_user=update user_info set name=?,email=?,password=?,first_name=?,last_name=?,department=?,user_id=?,mobile_num=?,no_of_books=?,role=? where email=?
search_book_name=select * from book where bname=?
issue_book=insert into issued_books values (?,?,?,?,?)
collect_book=delete from issued_books where book_id=?
